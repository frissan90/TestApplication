            $('#m1').mouseout(function () {
                $('#m1').animate(

                    {
                        width: 50,
                        height: 50
                    }, 50
                    )
            });

            $('#m2').mouseout(function () {
                $('#m2').animate(

                    {
                        width: 50,
                        height: 50
                    }, 50
                    )
