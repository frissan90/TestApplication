    <script type="text/javascript">
        $(document).ready(function () {
            $('#m1').mouseover(function () {
                $('#m1').animate(
                    
                    {
                        width: 100,
                        height: 100
                    }, 50
                    )
            });

            $('#m2').mouseover(function () {
                $('#m2').animate(

                    {
                        width: 100,
